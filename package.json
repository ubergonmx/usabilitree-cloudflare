{
  "name": "usabilitree",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "migrate-local": "wrangler d1 migrations apply usabilitree --local",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "types": "tsc --noEmit --pretty",
    "prettier": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
    "clean-local": "npx git-removed-branches --prune",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:dev": "drizzle-kit studio"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@react-email/components": "^0.0.26",
    "@react-email/render": "^1.0.2",
    "@t3-oss/env-nextjs": "^0.11.1",
    "arctic": "^2.2.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.36.0",
    "framer-motion": "^11.11.11",
    "lucide-react": "^0.454.0",
    "next": "^14.2.16",
    "next-themes": "^0.3.0",
    "nprogress": "^0.2.0",
    "oslo": "^1.2.1",
    "react": "^18",
    "react-dom": "^18",
    "sonner": "^1.6.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.5",
    "@cloudflare/workers-types": "^4.20241022.0",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.16",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/parser": "^8.12.0",
    "drizzle-kit": "^0.27.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-next-on-pages": "^1.13.5",
    "eslint-plugin-prettier": "^5.2.1",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vercel": "^32.3.0",
    "wrangler": "^3.83.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --cache --fix"
    ],
    "*.{json,md}": [
      "npx prettier --write"
    ]
  }
}
